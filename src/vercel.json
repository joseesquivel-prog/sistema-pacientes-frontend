{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://sistema-pacientes-backend-mxoi.onrender.com/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install"
}